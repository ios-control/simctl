{
  "name": "simctl",
  "version": "1.1.1",
  "description": "library for Xcode 8+ simctl utility on macOS",
  "repository": {
    "type": "git",
    "url": "https://github.com/ios-control/simctl.git"
  },
  "main": "simctl.js",
  "dependencies": {
    "shelljs": "^0.2.6",
    "tail": "^0.4.0"
  },
  "devDependencies": {
    "jasmine-node": "^1.14.5",
    "jscs": "^2.11.0",
    "jshint": "^2.9.1"
